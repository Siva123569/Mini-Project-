pipeline {
    agent any

    stages {
        stage('Clone') {
            steps {
                git 'https://github.com/nodejs/node.git'
            }
        }
        stage('Build') {
            steps {
                sh 'npm install'
            }
        }
        stage('Test') {
            steps {
                sh 'npm test'
            }
        }
    }
}

